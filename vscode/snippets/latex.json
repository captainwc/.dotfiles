{
    // Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
    // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
    // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
    // same ids are connected.
    // Example:
    // "Print to console": {
    // 	"prefix": "log",
    // 	"body": [
    // 		"console.log('$1');",
    // 		"$2"
    // 	],
    // 	"description": "Log output to console"
    // }
    "Section": {
        "prefix": "section",
        "body": "\\section{$1}\n\n$2"
    },
    "SubSection": {
        "prefix": "subsection",
        "body": "\\subsection{$1}\n\n$2"
    },
    "Insert Graph": {
        "prefix": "figure",
        "body": [
            "\\begin{figure}[h]",
            "   \\centering",
            "   \\includegraphics[width=1\\textwidth]{$1}",
            "   \\caption{$2}",
            "   \\label{fig:$3}",
            "\\end{figure}\n\n$4",
        ],
        "description": "Insert Figure Common Way"
    },
    "Insert Table": {
        "prefix": "table-3line",
        "body": [
            "\\begin{table}[h]",
            "\\centering",
            "\\caption{$1}",
            "\\label{tab:$2}",
            "\\begin{tabular}{cl}",
            "    \\toprule",
            "",
            "    \\midrule",
            "",
            "    \\bottomrule",
            "\\end{tabular}",
            "\\end{table}"
        ],
        "description": "Insert 3 line table"
    },
    "Insert usecase Table": {
        "prefix": "usetable",
        "body": [
            "\\begin{table}[h]",
            "\\centering",
            "\\caption{$1用例规格说明}",
            "\\label{tab:$2subusecase}",
            "\\begin{tabular}{p{0.17\\textwidth}<{\\centering}p{0.7\\textwidth}<{\\raggedright}}",
            "    \\toprule",
            "用例名称 & $3 \\\\\\",
            "    \\midrule",
            "参与角色 & \\\\\\",
            "前置条件 & \\\\\\",
            "基本事件流 & \\\\\\",
            "后置条件 & \\\\\\",
            "    \\bottomrule",
            "\\end{tabular}",
            "\\end{table}"
        ],
        "description": "Insert 3 line table"
    }
}